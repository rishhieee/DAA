def largestLocal(grid):
    n = len(grid)
    maxLocal = [[0] * (n - 2) for _ in range(n - 2)]

    for i in range(n - 2):
        for j in range(n - 2):
            maxLocal[i][j] = max(grid[i][j], grid[i][j + 1], grid[i][j + 2],
                                 grid[i + 1][j], grid[i + 1][j + 1], grid[i + 1][j + 2],
                                 grid[i + 2][j], grid[i + 2][j + 1], grid[i + 2][j + 2])

    return maxLocal
grid = [
    [9, 9, 8, 1],
    [5, 6, 2, 6],
    [8, 2, 6, 4],
    [6, 2, 2, 2]
]
print(largestLocal(grid)) 
