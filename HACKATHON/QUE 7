def unique_elements(arr):
    return list(set(arr))
input1 = [3, 7, 3, 5, 2, 5, 9, 2]
input2 = [-1, 2, -1, 3, 2, -2]
input3 = [1000000, 999999, 1000000]
print(unique_elements(input1))  
print(unique_elements(input2))  # Output: [-1, 2, 3, -2]
print(unique_elements(input3))  # Output: [1000000, 999999]
